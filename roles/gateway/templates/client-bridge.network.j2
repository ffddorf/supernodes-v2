[Match]
Name=br0

[Link]
RequiredForOnline=no

[Address]
{%- for addr in client_bridge_interface.ip_addresses %}
Address={{ addr }}
{%- endfor %}

[Network]
DHCPServer=yes

[DHCPServer]
PoolOffset=10
PoolSize=64000
EmitDNS=yes
ServerAddress=10.12.255.254/16
DNS={{ service_ipv4_address | ipaddr('address') }}
